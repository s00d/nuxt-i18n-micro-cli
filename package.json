{
  "name": "nuxt-i18n-micro-cli",
  "version": "1.0.3",
  "description": "⚡️ nuxt-i18n-micro-cli CLI Experience",
  "repository": "s00d/nuxt-i18n-micro-cli",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./bin/i18n.mjs"
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "i18n-micro": "./bin/i18n.mjs",
    "i18n-micro-cli": "./bin/i18n.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev:prepare": "unbuild --stub",
    "build": "unbuild",
    "build:stub": "unbuild --stub",
    "dev": "node ./bin/i18n.mjs",
    "dev:bun": "bun --bun ./bin/i18n.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "i18n": "node ./bin/i18n.mjs",
    "i18n-bun": "bun --bun ./bin/i18n.mjs",
    "prepack": "unbuild",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && pnpm build && pnpm test:dist",
    "test:dist": "node ./bin/i18n.mjs -h",
    "test:types": "tsc --noEmit"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.5.5",
    "@nuxt/kit": "^3.13.1",
    "@nuxt/schema": "^3.13.1",
    "@nuxt/test-utils": "^3.14.1",
    "@types/gettext-parser": "^4.0.4",
    "@types/http-proxy": "^1.17.15",
    "@types/node": "^20.16.5",
    "@types/prompts": "^2.4.9",
    "@types/semver": "^7.5.8",
    "@types/ws": "^8.5.12",
    "c12": "^1.11.2",
    "changelogen": "^0.5.5",
    "chokidar": "^3.6.0",
    "citty": "^0.1.6",
    "clipboardy": "^4.0.0",
    "colorette": "^2.0.20",
    "consola": "^3.2.3",
    "destr": "^2.0.3",
    "eslint": "^9.9.1",
    "execa": "^9.3.1",
    "fuse.js": "^7.0.0",
    "giget": "^1.2.3",
    "h3": "^1.12.0",
    "httpxy": "^0.1.5",
    "jiti": "^1.21.6",
    "listhen": "^1.7.2",
    "magicast": "^0.3.5",
    "mlly": "^1.7.1",
    "nuxt": "^3.13.1",
    "nypm": "^0.3.11",
    "ofetch": "^1.3.4",
    "pathe": "^1.1.2",
    "perfect-debounce": "^1.0.0",
    "pkg-types": "^1.2.0",
    "scule": "^1.3.0",
    "semver": "^7.6.3",
    "unbuild": "^2.0.0",
    "unplugin-purge-polyfills": "^0.0.5",
    "unws": "^0.2.4",
    "vue-tsc": "^2.1.6",
    "ws": "^8.18.0"
  },
  "optionalDependencies": {
    "fsevents": "~2.3.3"
  },
  "resolutions": {
    "nuxt": "^3.13.1"
  },
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "node": "^16.10.0 || >=18.0.0"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "gettext-parser": "^8.0.0",
    "glob": "^9.3.5",
    "node-fetch": "^3.3.2",
    "prompts": "^2.4.2"
  }
}
